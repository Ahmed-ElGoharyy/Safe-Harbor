{
  "manifest_version": 3,
  "name": "Safe Harbor Extension",
  "description": "An extension with Safe Harbor anonymization and domain-specific UI.",
  "version": "1.0",
  "action": {
    "default_popup": "safe-harbour.html",
    "default_icon": {
      "16": "logo_16.png",
      "32": "logo_32.png",
      "48": "logo_48.png",
      "128": "logo_128.png"
    }
  },
"permissions": ["activeTab"]
}
